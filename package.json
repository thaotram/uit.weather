{
    "name": "uit.weather",
    "version": "0.2.0",
    "description": "Edit Readme File",
    "main": "dist/server/app.js",
    "scripts": {
        "heroku-postbuild": "yarn run build",
        "style": "scss --watch style/index.scss:dist/render.css",
        "style-render":
            "scss --style compressed style/index.scss:dist/render.css",
        "build:server": "babel src/server -d dist/server",
        "build:client": "webpack",
        "build":
            "cross-env NODE_ENV=production npm-run-all build:server build:client",
        "start": "node dist/server/app",
        "run": "cross-env NODE_ENV=production node dist/server/app",
        "dev":
            "cross-env NODE_ENV=development nodemon src/server/app.js --exec babel-node",
        "test": "mocha --compilers js:babel-core/register",
        "chrome":
            "Start-Process \"chrome.exe\" \"ai.ai\" --unsafely-treat-insecure-origin-as-secure=http://ai.ai/"
    },
    "nodemonConfig": {
        "watch": ["src/server", "dist"]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/phamhongphuc/uit.weather.git"
    },
    "author": "",
